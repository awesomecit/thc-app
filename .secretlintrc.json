{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend",
      "allowMessagePatterns": [
        "/postgres:\\/\\/username:password@localhost/",
        "/postgres:\\/\\/user:pass@localhost/",
        "/postgres:\\/\\/myuser:mypassword@postgres/",
        "/postgres:\\/\\/thc_user:thc_password@localhost/",
        "/postgresql:\\/\\/username:password@localhost/"
      ]
    }
  ]
}
