{
  "$schema": "https://schemas.platformatic.dev/@platformatic/rabbitmq-hooks/0.6.3.json",
  "service": {
    "openapi": true
  },
  "watch": true,
  "module": "@platformatic/rabbitmq-hooks",
  "rabbitmq": {
    "url": "{PLT_THC_RABBITMQ_HOOKS_RABBITMQ_CONNECTION_URL}",
    "generateExchange": "{PLT_THC_RABBITMQ_HOOKS_RABBITMQ_GENERATE_EXCHANGE}",
    "exchanges": [
      {
        "name": "{PLT_THC_RABBITMQ_HOOKS_RABBITMQ_EXCHANGE_NAME_0}",
        "targetUrl": "{PLT_THC_RABBITMQ_HOOKS_RABBITMQ_TARGET_URL_0}",
        "queue": "{PLT_THC_RABBITMQ_HOOKS_RABBITMQ_TARGET_QUEUE_0}"
      }
    ]
  }
}